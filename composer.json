{
    "name": "angelobono/bono-llm-coding-agents",
    "description": "Bono Llm Coding Agents and Orchestration for PHP",
    "type": "project",
    "license": "BSD-3-Clause",
    "version": "1.0.0",
    "tags": [
        "php",
        "llm",
        "ollama",
        "orchestration",
        "agents",
        "generative-ai",
        "coding-agents",
        "psr"
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bono\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bono\\Tests\\": "test/"
        }
    },
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4",
        "ext-curl": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "^1.11.99.5",
        "laminas/laminas-stdlib": "^3.20",
        "monolog/monolog": "^3.9",
        "psr/simple-cache": "^3.0"
    },
    "require-dev": {
        "composer/composer": "^2.8.6",
        "filp/whoops": "^2.17.0",
        "phpunit/phpunit": "^10.5.45",
        "psalm/plugin-phpunit": "^0.19.2",
        "roave/security-advisories": "dev-master",
        "vimeo/psalm": "^6.8.8",
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "enable-codestandard": "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run",
        "cs-check": [
            "@enable-codestandard",
            "phpcs -s"
        ],
        "cs-fix": [
            "@enable-codestandard",
            "phpcbf"
        ],
        "serve": [
          "Composer\\Config::disableProcessTimeout",
          "php -S 0.0.0.0:8080 -t public/"
        ],
        "static-analysis": "psalm --stats",
        "static-analysis-update-baseline": "psalm --stats --update-baseline",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "docs-build": "mkdocs build"
    }
}
